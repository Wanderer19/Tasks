  <data name="SourceCodeCSharp" xml:space="preserve">
    <value>var array = new int[] {1, -9, 2, 3, 10};
var tmp = 0;
for (var i = 1; i &lt; array.Length; ++i){
	for (var j = 0; j &lt; array.Length - 1; ++j){
		if (array[j] &gt; array[j + 1]){
			tmp = array[j];
			array[j] = array[j + 1];
			array[j + 1] = tmp;
		}
	}
}
	</value>
